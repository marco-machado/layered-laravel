includes:
    - vendor/nunomaduro/larastan/extension.neon

parameters:
    # Set the level of analysis: 0 (basic) to 9 (very strict)
    level: max

    # Paths to analyze
    paths:
        - app
        - routes
        - database

    # Files and directories to exclude from analysis
    excludePaths:
        - bootstrap/cache/*
        - storage/*
        - vendor/*
